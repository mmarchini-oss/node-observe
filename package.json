{
  "name": "@mmarchini/observe",
  "version": "1.0.0",
  "description": "Inspector Protocol made easy",
  "main": "index.js",
  "scripts": {
    "test": "tape test/test-*.js",
    "coverage": "nyc tape test/test-*.js",
    "coverage-ci": "nyc --reporter=lcov tape test/test-*.js",
    "pkg": "pkg --target host --output dist/observe package.json",
    "linter": "eslint lib bin test"
  },
  "bin": "./bin/observe.js",
  "repository": {
    "type": "git",
    "url": "git@github.com/mmarchini/node-observe"
  },
  "keywords": [
    "inspector-protocol",
    "observability",
    "tracing",
    "profiling",
    "memory"
  ],
  "author": "mmarchini@netflix.com",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.8.0",
    "nyc": "^15.0.1",
    "pkg": "^4.4.9",
    "tape": "^4.13.2"
  },
  "pkg": {
    "assets": [
      "bin/*.js",
      "lib/**/*.js"
    ]
  },
  "dependencies": {
    "commander": "^5.0.0",
    "ws": "^7.2.3"
  }
}
